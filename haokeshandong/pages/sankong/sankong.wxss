/* pages/sankong/sankong.wxss */
.banner-swiper {
  width: 100%;
  height: 400rpx;
  overflow: hidden;
}
swiper {
  display: block;
  height: 400rpx;
  position: relative;
}
.slide-image {
  width: 97%;
  display: block;
  margin: 0 auto;
  height: 360rpx;
  margin-top:20rpx;
  border-radius: 10rpx;
}
.active{
  margin-top:0rpx;
  height: 400rpx;
  border-radius:10rpx; 
}
.box{
  margin: 20px;
  padding: 20px;
  border: 1px;
  background-color: skyblue;
}
.text{
  text-indent: 30px;
}
navigator{
  margin: 5px;
  font-size: 20px;
}
.li{
  display: flex;
  flex-direction: row;
  margin: 5px 5px;
  padding: 5px 5px;
}
.myleft{
  margin-right: 10px;
}
.mycenter{
  flex: 1;
}
.image{
  width: 10rpx;
  height: 10rpx;
}
.myright{
  width: 40rpx;
  height: 40rpx;
  margin-top: 5px;
}